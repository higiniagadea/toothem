<h1>Listando prestaciones</h1>

<table>
  <tr>
    <th>Codigo</th>
    <th>Descripcion</th>
    <th>Consultorio</th>
  </tr>

<% @prestaciones.each do |prestacion| %>
  <tr>
    <td><%=h prestacion.codigo %></td>
    <td><%=h prestacion.descripcion %></td>
    <td><%=h prestacion.consultorio_id %></td>
    <td><%= link_to 'Ver', prestacion %></td>
    <td><%= link_to 'Editar', edit_prestacion_path(prestacion) %></td>
    <td><%= link_to 'Borrar', prestacion, :confirm => '¿Está seguro? No podrá deshacer esta acción', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Nueva prestacion', new_prestacion_path %>