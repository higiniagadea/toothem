
<% unless @tratamientos.blank? %>
      <table style=" text-align: left "class="table-list">
  <tr>

    <th>Fecha</th>
    <th>Profesional</th>
    <th>Estado</th>
    <th>Acciones</th>
    </tr>
      <% @tratamientos.each do |tratamiento| %>
  <tr>
    <td><%=h tratamiento.fecha %></td>
    <td><%=h tratamiento.profesional_id %></td>
    <td><%=h tratamiento.estado_tratamiento.nombre %></td>
    <td><%= link_to image_tag('magnifier_zoom_in.png'), tratamiento, :title => "Ver", :id=> 'ver' %></td>
    <td><%= link_to image_tag('pencil.png'), edit_tratamiento_path(tratamiento, :paciente_id => @paciente), :title => 'Editar', :id=> 'editar' %></td>
    <td><%= link_to image_tag('cancel.png'),eliminar_tratamiento_path(tratamiento, :paciente_id => @paciente.id), :confirm => 'Esta seguro de eliminar el tratamiento?', :title=> 'Eliminar' %></td>

  </tr>

<%end%>
  <tr><td>
  <%= will_paginate @tratamientos, :previous_label=>'Anterior', :next_label => 'Siguiente' %>
    </td></tr>
      </table>
<% end%>
