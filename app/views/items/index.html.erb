

<table class="table-list">
  <tr>
    <th>Nombre</th>
    <th>Url</th>
    <th>Metodo http</th>
    <th>Orden</th>
    <th>Estatico</th>
  </tr>

<% @items.each do |item| %>
  <tr class="<%= cycle('','odd') %>">
    <td><%=h item.nombre %></td>
    <td><%=h item.url %></td>
    <td><%=h item.metodo_http %></td>
    <td><%=h item.orden %></td>
    <td><%=h item.estatico %></td>
    <td><%= link_to image_tag('magnifier_zoom_in.png'), item %>
        <%= link_to image_tag('pencil.png'), edit_item_path(item) %>
        <%= link_to image_tag('cancel.png'), item, :confirm => 'Está seguro? No podrá deshacer esta acción', :method => :delete unless item.estatico? %></td>
  </tr>
<% end %>
</table>

<br />
<%= will_paginate @items, :previous_label=>'Anterior', :next_label => 'Siguiente' %>
<%= link_to 'Nuevo item', new_item_path %>