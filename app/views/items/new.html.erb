<h1>Nuevo item</h1>

<% form_for(@item) do |f| %>
  <%= f.error_messages %>
<ul id="form" >
  <li>
    <label for="nombre">Nombre </label>
    <%= f.text_field :nombre %>
  </li>
  <li>
    <label for="url">URL </label>
    <%= f.text_field :url %>
  </li>
  <li>
    <label for="metodo_http">Metodo_http </label>
    <%= f.text_field :metodo_http %>
  </li>
  <li>
    <label for="orden">Orden </label>
    <%= f.text_field :orden %>
  </li>
  <li>
    <label for="estatico">Estatico</label>
    <%= f.check_box :estatico %>
  </li>
</ul>
  <p>
    <%= f.submit 'Crear' %>
  </p>
<% end %>

<%= link_to 'Volver', items_path %>