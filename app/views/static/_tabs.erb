<ul id="tabs">
<%unless session[:user_menu].blank? %>
  <li id="consultorio">
    <a><%=h session[:consultorio] %></a>
    
  </li>
  <% session[:user_menu].each do |item| %>
  <li <%if controller.controller_name.downcase == item.nombre.downcase %> class="selected" <%end%>
            onclick="javascript:location.href='<%= item.url  %>?item_selected=<%= item.id %>'">
    <% if item.nombre.downcase == 'logout' %>
    Salir</li>
    <% else %>
        <%=h item.nombre.humanize %></li>
    <% end %>

  <%end%>
  
<%end%>
</ul>


<script type="text/javascript">
      // Only create tooltips when document is ready
      $(document).ready(function()
      {
        // Use the each() method to gain access to each of the elements attributes
        $('#consultorio a').each(function()
        {
          $(this).qtip(
          {
            content: '<%= render :partial => 'consultorios/consultorios_usuario' %>', // Give it some content
            position: 'topRight', // Set its position
            hide: {
              fixed: true // Make it fixed so it can be hovered over
            },
            style: {
              padding: '5px 15px', // Give it some extra padding
              name: 'blue' // And style it with the preset dark theme
            }
          });
        });
      });
    </script>
