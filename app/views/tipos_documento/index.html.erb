<h1>Listado de tipos documento</h1>

<table id="table-list">
  <tr>
    <th>Descripcion</th>
  </tr>

<% @tipos_documento.each do |tipo_documento| %>
  <tr>
    <td><%=h tipo_documento.descripcion %></td>
    <td><%= link_to 'Ver', tipo_documento %></td>
    <td><%= link_to 'Editar', edit_tipo_documento_path(tipo_documento) %></td>
    <td><%= link_to 'Borrar', tipo_documento, :confirm => 'Esta seguro?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Nuevo tipo_documento', new_tipo_documento_path %>