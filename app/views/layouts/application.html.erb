<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">

<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
        <%= stylesheet_link_tag "main" %>
        <%= javascript_include_tag :defaults %>
        <%= javascript_include_tag 'qtip.js','jquery.boxy.js' %>
        <link rel="stylesheet" type="text/css" href="../style-projects-jquery.css" />

        <title>Toothem &copy;</title>

    </head>
    <body>
        <div id="wrapper">
            <div id="container">
                <div id="header">
                  <div id="logo" style="float:left">
                  <img src="/images/toothem-logo.png" alt="tlogo">  
                  </div>


                  <div id="tabs">
                    <ul id="nav">
                      <li>Inicio</li>
                      <li class="selected">Pacientes</li>
                      
                      <li>Menu 2</li>
                      <li onclick="location.href='/logout'">Salir</li>
                    </ul>
                  </div>

                </div>
                <div id="content">
                  <div id="main-content">
                    <div id="title"><%= @title %></div>
                    <%if flash[:notice]%>
                    <div id ="notice"><%=  flash[:notice]%></div>
                    <%end%>
                    <%if flash[:error]%>
                    <div id ="error"><%=  flash[:error]%></div>
                    <%end%>
                    <%= yield %>
                  </div>
                  <div id="right-content">
                    <%= render :partial => "static/menu.erb" %>
                  </div>
                  <div style="clear:both;"></div>

                    



                </div>
                <div id="pre-footer">
                   

                </div>
            </div>

        </div>
        <div id="footer">
            &copy; Toothem
        </div>
       

    </body>

</html>
