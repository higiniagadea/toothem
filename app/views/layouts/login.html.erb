<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">

<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
        <%#= stylesheet_link_tag "main" %>
        <%#= javascript_include_tag :defaults %>

        <%= javascript_include_tag :defaults, 'jquery.validate.min.js', 'messages_es.js',
      "jquery.ui.widget.js" ,"jquery.ui.button.js","jquery.livequery.js", 'app.js','qtip.js',
      'jquery.safeenter.js', 'more-show.js','jquery.easing.pack.js','jquery.mousewheel.pack.js','jquery.fancybox.pack.js',
      'jquery.autocomplete.js', 'jquery.clickmenu.js','jquery.maskedinput.min.js'%><!--,-->
    <%= stylesheet_link_tag "main",'jquery-ui','ui-button','fancybox'%>
        
       
        

        <title>Toothem &copy;</title>
    </head>
    <body>
        <div id="wrapper">
            <div id="container">
                <div id="header">
                  <div id="logo" style="float:left">
                  logo clinica
                  </div>
                 

                </div>
              <br/>
                <div id="content">
                  <div id="main-content">
          <%if flash[:notice]%>
          <div class="notice-error">
              <div  id="notice" class="ui-state-highlight ui-corner-all" style=" padding: 0 .7em;">
                <p><span class="ui-icon ui-icon-info" style="float: left; margin-right: .3em;"></span>
                  <%= flash[:notice] %>
                </p>
              </div>
          </div>
          <br/>

            <%end%>
           <%if flash[:error]%>
          <div class="notice-error">
          <div id="error" class="notice-error ui-state-error ui-corner-all" style="padding: 0 .7em;">
            <p><span class="ui-icon ui-icon-alert" style="float: left; margin-right: .3em;"></span>
            <%= flash[:error]%>
            </p>
          </div>
          </div>
          <br/>


          <%end%>
                    <%= yield %>
                  </div>
                  
                  

                    



                </div>
                <div id="pre-footer">
                    

                </div>


            </div>

        </div>
        <div id="footer">
            &copy; Toothem
        </div>


    </body>

</html>
