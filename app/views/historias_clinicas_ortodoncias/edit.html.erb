<fieldset class="ui-widget ui-widget-content ui-corner-top">
  <legend class="ui-widget ui-widget-content ui-corner-all ui-widget-header">Historia Clinica</legend>

<% form_for(:historia_clinica_ortodoncia, @historia_clinica_ortodoncia,
            :url => paciente_historia_clinica_ortodoncia_url(@paciente), :html=> {:method=> :put}) do |f| %>
<%= f.error_messages %>
<div id="pacientes-screen">
  <div id="pacientes-options">
    <div id="pacientes-nombre">
      <%=h @paciente.nombre %>
    </div>
    <div id="pacientes-actions">
       <tr><%= f.submit 'Actualizar', :class => 'submit ui-button ui-widget ui-state-default ui-corner-all' %></tr>
        <table>
        <tr>
         <%= link_to image_tag('magnifier_zoom_in.png'), paciente_historia_clinica_ortodoncia_path(@paciente), :title => 'Ver' %> |
          <%= link_to image_tag('printer.png'), imprimir_paciente_historia_clinica_ortodoncia_path(@paciente), :title => 'Imprimir', :popup => ['Imprimir', 'height=300,width=600'] %>
        </tr>
      </table>
      </div>
     </div>

