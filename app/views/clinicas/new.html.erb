<h1>Nueva clinica</h1>

<% form_for(@clinica) do |f| %>
  <%= f.error_messages %>
<ul id="form">
  <li>
    <label for="nombre">Nombre </label>
    <%= f.text_field :nombre %>
  </li>

  <li>
    <label for="direccion">Direccion </label>
    <%= f.text_field :direccion %>
  </li>

  <li>
    <label for="cuit">Cuit </label>
    <%= f.text_field :cuit %>
  </li>

</ul>

  <p>
    <%= f.submit 'Crear' %>
  </p>
<% end %>

<%= link_to 'Volver', clinicas_path %>