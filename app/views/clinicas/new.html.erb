<fieldset class="ui-widget ui-widget-content ui-corner-top">
  <legend class ="ui-widget ui-widget-content ui-corner-all ui-widget-header">Nueva Clinica</legend>
<% form_for(@clinica) do |f| %>
  <%= f.error_messages  :id => 'error', :header_message => '', :message => 'OcurriÃ³ un error al intentar guardar el registro'  %>
<table id="form">
  <tr>
  <td>
    <label for="nombre">Nombre </label></td>
  <td><%= f.text_field :nombre, :class => 'normal' %>
  </td>
  </tr>

  <tr>
  <td>
    <label for="direccion">Direccion </label></td>
  <td><%= f.text_field :direccion, :class => 'normal' %>
  </td>
  </tr>

  <tr>
  <td>
    <label for="cuit">Cuit </label></td>
  <td><%= f.text_field :cuit, :class => 'normal' %>
  </td>
  </tr>

  <td>
    <label></label><%= f.submit 'Crear', :class => 'submit ui-button ui-widget ui-state-default ui-corner-all' %>
  </td>
</table>
</fieldset>
<% end %>
