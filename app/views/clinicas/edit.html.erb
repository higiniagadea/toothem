<h1>Editando clinica</h1>

<% form_for(@clinica) do |f| %>
  <%= f.error_messages %>
<ul id="form">
  <li>
    <label for="nombre">Nombre </label>
    <%= f.text_field :nombre %>
  </li>

  <li>
    <label for="direccion">Direccion </lablel>
    <%= f.text_field :direccion %>
  </li>

  <li>
    <label for="cuit">Cuit </label>
    <%= f.text_field :cuit %>
  </li>
</ul>
  <p>
    <%= f.submit 'Actualizar' %>
  </p>
<% end %>

<%= link_to 'Ver', @clinica %> |
<%= link_to 'Volver', clinicas_path %>