<table class="table-list">
  <tr>
  
    <th>Localidad</th>
    <th>Paciente</th>
  </tr>

<% @fichas.each do |ficha| %>
  <tr>
   
    <td><%=h ficha.localidad_id %></td>
    <td><%=h ficha.paciente.nombre %></td>
    
    <td><%= link_to image_tag('pencil.png'), edit_ficha_path(ficha), :title => 'Editar' %></td>
    <td><%= link_to 'Eliminar', ficha, :confirm => 'Esta seguro?', :method => :delete, :title => 'Eliminar' %></td>
  </tr>
<% end %>
</table>

<br />
