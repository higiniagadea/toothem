<fieldset class="ui-widget ui-widget-content ui-corner-top">
  <legend class="ui-widget ui-widget-content ui-corner-all ui-widget-header">Obras Sociales</legend>

<table class="table-list">
  <tr>
    <th>Nombre</th>
    <th>Auditoria previa</th>
    <th>Auditoria post</th>
    <th>Nro max codigos por mes</th>
    <th>Incluye ficha</th>
    
    
  </tr>

<% @obras_sociales.each do |obra_social| %>
  <tr class="<%= cycle('','odd') %>">
    <td><%=h obra_social.nombre %></td>
    <td><%=h boolean_desc(obra_social.auditoria_previa)%></td>
    <td><%=h boolean_desc(obra_social.auditoria_post) %></td>
    <td><%=h obra_social.nro_max_codigos_por_mes %></td>
    <td><%=h boolean_desc(obra_social.incluye_ficha) %></td>
    
    
    <td><%= link_to image_tag('magnifier_zoom_in.png'), obra_social, :title => "Ver", :id => 'siete_' + obra_social.id.to_s  %>
        <%= link_to image_tag('pencil.png'), edit_obra_social_path(obra_social), :title => "Editar", :id=> 'seis_' + obra_social.id.to_s   %>
        <%= link_to image_tag('cancel.png'), obra_social, :confirm => '¿Está seguro? No podrá deshacer esta acción', :method => :delete, :title => "Eliminar" %>
        <%= link_to image_tag('money_dollar.png'), new_obra_social_arancel_path(obra_social), :title => "Aranceles", :id => 'nueve_' + obra_social.id.to_s%>
        <%= link_to 'buscar', busqueda_obra_social_aranceles_path(obra_social, @aranceles) %>
        </td>
  </tr>

   <script type="text/javascript">
  $(document).ready(function(){
 $('#siete_<%= obra_social.id.to_s %>').fancybox(
        {
      'hideOnContentClick': false,
      'titleShow': false

    });

    $('#seis_<%= obra_social.id.to_s %>').fancybox(
        {
      'hideOnContentClick': false,
      'titleShow': false

    });
 $('#nueve_<%= obra_social.id.to_s %>').fancybox(
        {
      'hideOnContentClick': false,
      'titleShow': false

    });

    });
    </script>



<% end %>
</table>
</fieldset>

<%= will_paginate @obras_sociales, :previous_label=>'Anterior', :next_label => 'Siguiente' %>


