<h1>Editando el estado_laboral</h1>

<% form_for(@estado_laboral) do |f| %>
  <%= f.error_messages %>
<ul id="form">
  <li>
     <label for="descripcion">Descripcion </label><br />
    <%= f.text_field :descripcion %>
  </li>
 </ul>
  <p>
    <%= f.submit 'Actualizar' %>
  </p>
<% end %>

<%= link_to 'Ver', @estado_laboral %> |
<%= link_to 'Volver', estados_laborales_path %>