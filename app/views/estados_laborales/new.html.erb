<h1>Nuevo estado laboral</h1>

<% form_for(@estado_laboral) do |f| %>
  <%= f.error_messages %>
<ul id="form">
  <li>
     <label for="descripcion">Descripcion </label> 
    <%= f.text_field :descripcion %>
  </li>
 </ul>
  <p>
    <%= f.submit 'Crear' %>
  </p>
<% end %>

<%= link_to 'Volver', estados_laborales_path %>