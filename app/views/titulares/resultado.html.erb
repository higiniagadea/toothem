<fieldset class="ui-widget ui-widget-content ui-corner-top">
  <legend class="ui-widget ui-widget-content ui-corner-all ui-widget-header">Titular</legend>

<table class="table-list">
 
    <tr>
      <th>Nombre</th>
      <th>Matricula</th>
      <th>Obra social</th>
    </tr>

  <% @titulares.each do |titular| %>
    <tr class="<%=  cycle('par','impar')%>">
      <td><%=h titular.nombre %></td>
      <td><%=h titular.matricula %></td>
      <td><%=h titular.obra_social_id %></td>
      <td><%= link_to 'Ver', titular %></td>
      <td><%= link_to 'Editar', edit_titular_path(titular) %></td>
      <td><%= link_to 'Borrar', titular, :confirm => '¿Está seguro? No podrá deshacer esta acción', :method => :delete %></td>
    </tr>   
  <% end %>
    
</table>
</fieldset>
