<h1>Nuevo profesional</h1>

<% form_for(@profesional) do |f| %>
  <%= f.error_messages %>
<ul id="form" >
  <li>
    <label for="nombre">Nombre  </label>
    <%= f.text_field :nombre %>
  </li>
  <li>
    <label for="tipo_documento">Tipo documento </label>
    <%= f.collection_select(:tipo_documento_id, TipoDocumento.all, :id, :descripcion) %>
    
  </li>
  <li>
    <label for="documento">Documento </label>
    <%= f.text_field :documento %>
  </li>
  <li>
    <label for="matricula_profesional">Matricula Profesional </label>
    <%= f.text_field :matricula_profesional %>
  </li>
  <li>
    <label for="direccion">Direccion </label> 
    <%= f.text_field :direccion %>
  </li>
  <li>
    <label for="telefono">Telefono </label> 
    <%= f.text_field :telefono %>
  </li>
  <li>
    <label for="movil">Movil </label>
    <%= f.text_field :movil %>
  </li>
  <li>
    <label for="email">email </label> 
    <%= f.text_field :email %>
  </li>
  <li>
    <label for="fecha_nacimiento">Fecha de Nacimiento </label> 
    <%= f.date_select :fecha_nacimiento %>
  </li>
  <li>
    <label for="fecha_ingreso">Fecha Ingreso </label>
    <%= f.date_select :fecha_ingreso %>
  </li>
  <li>
    <label for="consultorio_id">Consultorio </label>
   <%= f.text_field :consultorio_id %>
 
  </li>
  <li>
    <label for="estado_laboral_id">Estado Laboral </label> 
    <%= f.collection_select :estado_laboral_id, EstadoLaboral.all, :id, :descripcion %>
  </li>
</ul>
  <p>
    <%= f.submit 'Crear' %>
  </p>
<% end %>

<%= link_to 'Volver', profesionales_path %>