<table class="table-list">
  <tr>
    <th>Nombre</th>
    <th>Consultorio</th>
    <th>Matr√≠cula Profesional</th>
   
  </tr>

<% @profesionales.each do |profesional| %>
  <tr class="<%= cycle('','odd') %>">
    <td><%=h profesional.nombre %></td>
     <td><%=h profesional.consultorio.nombre %></td>
      <td><%=h profesional.matricula_profesional %></td>

    <td><%= link_to image_tag('magnifier_zoom_in.png'), profesional, :title => 'Ver' %></td>
    <td><%= link_to image_tag('pencil.png'), edit_profesional_path(profesional), :title => 'Editar' %></td>
    <td><%= link_to image_tag('cancel.png'), profesional, :confirm => 'Esta seguro?', :method => :delete, :title => 'Eliminar'%></td>
    
  </tr>
<% end %>
</table>

<br />
<%= will_paginate @profesionales, :previous_label=>'Anterior', :next_label => 'Siguiente' %>